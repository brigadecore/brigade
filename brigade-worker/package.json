{
  "name": "brigade-worker",
  "version": "1.2.1",
  "description": "Brigade Worker",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write ./{src,test}/**/*.ts",
    "prestart": "node prestart.js",
    "build-start": "yarn build && yarn start",
    "start": "node --no-deprecation ./dist/index.js",
    "test": "mocha --require ts-node/register --recursive ./test/**/*.ts",
    "build": "tsc",
    "brigade": "yarn start"
  },
  "author": "The Steel Thread Team",
  "license": "Apache-2.0",
  "repository": "https://github.com/brigadecore/brigade",
  "devDependencies": {
    "@types/byline": "^4.2.31",
    "@types/chai": "^4.0.1",
    "@types/mocha": "^5.2.6",
    "@types/mock-require": "^2.0.0",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^11.9.6",
    "@types/sinon": "^7.0.11",
    "@types/ws": "6.0.1",
    "chai": "^4.1.0",
    "mocha": "^9.1.3",
    "mock-require": "^3.0.3",
    "prettier": "^1.9.1",
    "rimraf": "^2.6.2",
    "sinon": "^7.3.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@brigadecore/brigadier": "^1.5.0",
    "@kubernetes/client-node": "^0.10.1",
    "byline": "^5.0.0",
    "child-process-promise": "^2.2.1",
    "module-alias": "^2.2.0",
    "pretty-error": "^4.0.0",
    "ulid": "^0.2.0"
  },
  "resolutions": {
    "lodash": "^4.17.17"
  }
}
